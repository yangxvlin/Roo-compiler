array[10] integer testArray;

procedure main () 
integer i, length;
testArray a;
{
     i <- 0;
     length <- 10;
     while i < length do
         a[i] <- i * i;
         i <- i + 1;
     od
     call aMean(a, length);
}

procedure aMean (testArray a, integer length) 
integer x, i;
{
    i <- 0;
    x <- 0;
    while i < length do
         x <- x + a[i];
         i <- i + 1;
    od
    x <- x / 10;
    write x;
}